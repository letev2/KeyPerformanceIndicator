@model KpiNew.Dto.AdminDto
@{
    Layout = "_DashBoard"; 
 }



<dl class="row">

    <dt class="col-sm-2">
        FirstName:
    </dt>
    <dd class="col-sm-10">
        @Model.FirstName
    </dd>

    <dt class="col-sm-2">
        LastName:
    </dt>

      <dd class="col-sm-10">
        @Model.LastName
    </dd>

      <dt class="col-sm-2">
        Address:
    </dt>

      <dd class="col-sm-10">
        @Model.Address
    </dd>

      <dt class="col-sm-2">
        Gender:
    </dt>

      <dd class="col-sm-10">
        @Model.Gender
    </dd>

   <dt class="col-sm-2">
        Admin Photo:
    </dt>
    <dd class="col-sm-10">
        <img class="card-img" style="height:200px; width:200px" src="~/adminPhotos/@Model.AdminImage"/>
    </dd>


</dl>

<button style="background-color:darkblue;"><a asp-action="Index">Back to List</a></button>