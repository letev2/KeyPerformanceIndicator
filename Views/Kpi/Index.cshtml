
@model IEnumerable<KpiNew.Dto.KpiDto>

@{
    Layout = "_DashBoard"; 
 }

<div class="card-header"  style="background-color:green;">
    <div class="card-title">
        <h1  class=" justify-contest-center">Kpi</h1>
    </div>
</div>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Id)
            </th>


            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>

            <th>
                @Html.DisplayNameFor(model => model.Rating)
            </th>
           

             


             





            <th>
                Update
            </th>
            <th>
                Details
            </th>
            <th>
                Delete
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var kpi in Model)
        {
            <tr>
                 <td>
                    @Html.DisplayFor(modelItem => kpi.Id)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => kpi.Name)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => kpi.Rating)
                </td>
              

                <td>
                    <a asp-action="Update" asp-route-id="@kpi.Id">Update</a>
                </td>
                <td>
                    <a asp-action="Details" asp-route-id="@kpi.Id">Details</a>
                </td>
                <td>
                    <a asp-action="Delete" asp-controller="Kpi" asp-route-id="@kpi.Id">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>
<button style="background-color:wheat;"><a asp-controller="Kpi" asp-action="Create">Register Kpi</a> </button>

