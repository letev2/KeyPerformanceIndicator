@model IEnumerable<KpiNew.Dto.KpiDto>
@{
  Layout = "_DashBoard";
}

    
<div class="card-header"  style="background-color:green;">
    <div class="card-title">
        <h1  class=" justify-contest-center">Kpi List</h1>
    </div>
</div>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Id)
            </th>


            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>

            <th>
                @Html.DisplayNameFor(model => model.Rating)
            </th>
           
               <th>
                @Html.DisplayNameFor(model => model.Employee)
            </th>

        </tr>
    </thead>
    <tbody>
        @foreach (var kpi in Model)
        {
            <tr>
                 <td>
                    @Html.DisplayFor(modelItem => kpi.Id)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => kpi.Name)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => kpi.Rating)
                </td>
                  <td>
                    @Html.DisplayFor(modelItem => kpi.Employee)
                </td>
            </tr>
        }
    </tbody>
</table>
<button style="background-color:wheat;"><a asp-controller="Admin" asp-action="AdminDashBoard">Back to Home</a> </button>
